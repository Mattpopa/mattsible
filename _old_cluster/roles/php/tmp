./configure
--build=x86_64-redhat-linux-gnu
--host=x86_64-redhat-linux-gnu
--prefix=/usr
--exec-prefix=/usr
--bindir=/usr/bin
--sbindir=/usr/sbin
--sysconfdir=/etc
--datadir=/usr/share
--includedir=/usr/include
--libdir=/usr/lib64
--libexecdir=/usr/libexec
--localstatedir=/var
--sharedstatedir=/usr/com
--mandir=/usr/share/man
--infodir=/usr/share/info
--cache-file=../config.cache
--with-libdir=lib64
--with-config-file-path=/etc
--with-config-file-scan-dir=/etc/php.d
--disable-debug
--with-pic
--disable-rpath
--with-layout=GNU
--with-apxs2=/usr/sbin/apxs
--with-bz2
--enable-ctype
--with-curl
--enable-dom
--enable-libxml
--with-gmp
--enable-hash
--with-gd
--with-jpeg-dir
--with-png-dir
--enable-json
--enable-mbstring
--with-mcrypt
--with-mysqli=/usr/bin/mysql_config
--with-pcre-regex
--enable-pdo
--with-pdo-mysql
--enable-posix
--enable-simplexml
--enable-sockets
--with-zlib
--disable-bcmath
--disable-calendar
--disable-exif
--without-gettext
--without-mhash
--with-openssl
--without-pspell
--without-readline
--disable-sysvsem
--disable-sysvshm
--disable-sysvmsg
--disable-shmop
--enable-soap
--without-tidy
--enable-zip
--with-imap
--with-imap-ssl
--with-kerberos
--enable-opcache
--enable-fpm
--with-freetype-dir=/usr
--enable-gd-native-ttf
--enable-intl
